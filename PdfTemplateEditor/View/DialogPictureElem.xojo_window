#tag Events cvPicture
	#tag Event
		Sub Paint(g As Graphics, areas() As Rect)
		  If Self.Elem <> Nil And Self.elem.Pic <> Nil Then
		    Var scale As Double = g.Width / Self.Elem.Pic.Width
		    g.DrawPicture(Self.Elem.Pic, 0, 0, g.Width, g.Height, 0, 0, Self.Elem.Pic.Width, Self.Elem.Pic.Height)
		  End
		  
		  g.DrawingColor = Color.Gray
		  g.DrawRectangle(0, 0, g.Width, g.Height)
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub DoublePressed(x As Integer, y As Integer)
		  OpenFile
		End Sub
	#tag EndEvent
#tag EndEvents
