#tag WindowCode
	#tag Method, Flags = &h21
		Private Sub Constructor()
		  // Calling the overridden superclass constructor.
		  Super.Constructor
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Constructor(elem as PdfTemplate.CanvasElementPicture)
		  Self.Elem = elem
		  
		  // Calling the overridden superclass constructor.
		  Super.Constructor
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub OpenFile()
		  Var f As FolderItem = FolderItem.ShowOpenFileDialog("image/png, image/jpeg")
		  
		  If f <> Nil And f.Exists Then
		    Self.Elem.Pic = Picture.Open(f)
		    cvPicture.Refresh
		  End
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		DialogOk As Boolean = False
	#tag EndProperty

	#tag Property, Flags = &h21
		Private Elem As PdfTemplate.CanvasElementPicture
	#tag EndProperty


#tag EndWindowCode

#tag Events btnOk
	#tag Event
		Sub Pressed()
		  Self.DialogOk = True
		  Self.Close
		End Sub
	#tag EndEvent
#tag EndEvents
