#tag Events ListBox1
	#tag Event
		Sub Opening()
		  PopulateFontsList()
		End Sub
	#tag EndEvent
	#tag Event
		Function PaintCellBackground(g As Graphics, row As Integer, column As Integer) As Boolean
		  If row Mod 2 = 0 Then
		    g.DrawingColor = &cEBEBEB00
		    g.FillRectangle(0, 0, g.Width, g.Height)
		  End
		End Function
	#tag EndEvent
	#tag Event
		Sub SelectionChanged()
		  Using PdfTemplate
		  
		  If Me.SelectedRowIndex >= 0 And Me.SelectedRowIndex < Me.RowCount Then
		    Self.SelectedFontDef = PdfFont(Me.RowTagAt(Me.SelectedRowIndex))
		    tbFontColor.Text = Self.SelectedFontDef.DrawingColor.ToString
		    tbFontName.Text = Self.SelectedFontDef.FontName
		    tbFontSize.Text = Self.SelectedFontDef.Size.ToString
		    tbName.Text = Self.SelectedFontDef.Name
		    cbBold.Value = Self.SelectedFontDef.Bold
		    cbItalic.Value = Self.SelectedFontDef.Italic
		    cbUnderlined.Value = Self.SelectedFontDef.Underlined
		  End
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events btnNew
	#tag Event
		Sub Pressed()
		  Using PdfTemplate
		  
		  Var fnt As New PdfFont
		  fnt.Size = 14
		  fnt.FontName = "Arial"
		  fnt.Name = "<Name>"
		  App.PdfSvc.FontDefs.Add(fnt)
		  PopulateFontsList
		  ListBox1.SelectedRowIndex = ListBox1.LastAddedRowIndex
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events btnDelete
	#tag Event
		Sub Pressed()
		  If MessageAskBox.Show("Löschen", "Soll die gewählte Schriftdefinition entfernt werden?", MessageDialog.IconTypes.Question) = 1 Then
		    App.PdfSvc.RemoveFontDef(tbName.Text)
		    PopulateFontsList
		  End
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events btnSelectColor
	#tag Event
		Sub Pressed()
		  Var cp As New DesktopColorPicker
		  AddHandler cp.ColorSelected, AddressOf ColorSelected
		  cp.Show(SelectedFontDef.DrawingColor, "Farbe")
		End Sub
	#tag EndEvent
#tag EndEvents
