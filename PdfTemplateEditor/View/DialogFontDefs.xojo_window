#tag WindowCode
	#tag Method, Flags = &h21
		Private Sub ColorSelected(sender as object, selectedColor as Color)
		  tbFontColor.Text = selectedColor.ToString
		  tbFontColor.TextColor = selectedColor
		  SelectedFontDef.DrawingColor = selectedColor
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Constructor()
		  Using PdfTemplate
		  
		  Self.SelectedFontDef = New PdfFont
		  
		  // Calling the overridden superclass constructor.
		  Super.Constructor
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub PopulateFontsList()
		  Using PdfTemplate
		  
		  ListBox1.RemoveAllRows
		  
		  For Each fntDef As PdfFont In App.PdfSvc.FontDefs
		    ListBox1.AddRow(fntDef.Name)
		    ListBox1.RowTagAt(ListBox1.LastAddedRowIndex) = fntDef
		  Next
		  
		  ListBox1.RowSelectedAt(0) = true
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		DialogOk As Boolean = False
	#tag EndProperty

	#tag Property, Flags = &h21
		Private SelectedFontDef As PdfTemplate.PdfFont
	#tag EndProperty


#tag EndWindowCode

