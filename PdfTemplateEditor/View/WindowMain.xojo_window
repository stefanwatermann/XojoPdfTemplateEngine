#tag Events CanvasRulerV
	#tag Event
		Sub Paint(g As Graphics, areas() As Rect)
		  Using PdfTemplate
		  
		  // Hintergrund
		  g.DrawingColor = &cD6D6D600
		  g.FillRectangle(0, 0, g.Width, g.Height)
		  
		  // Lineal
		  g.DrawingColor = &c91919100
		  g.FontName = "Arial"
		  g.FontSize = 9
		  For m As Integer = App.PdfSvc.GridSizeMm To App.PdfSvc.PxToMm(g.Width) Step App.PdfSvc.GridSizeMm* 2
		    Var x As Integer = App.PdfSvc.MmToPx(m)
		    g.DrawLine(x, 0, x, g.Height)
		    g.DrawText(Str(m), x + 5, g.Height - 3)
		  Next
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ToolbarMainWindow1
	#tag Event
		Sub Pressed(item As DesktopToolbarItem)
		  Select Case item.Name
		    
		  Case "tbNew"
		    NewTemplate()
		    
		  Case "tbOpenFile"
		    OpenTemplateFile()
		    
		  Case "tbSaveFile"
		    SaveTemplateFile()
		    
		  Case "tbEditElem"
		    EditCanvasElement()
		    
		  Case "tbDeleteElem"
		    RemoveCanvasElement()
		    
		  Case "tbFontDefs"
		    ShowFontDefs()
		    
		  Case "tbToPdf"
		    CreatePdf()
		    
		  End
		End Sub
	#tag EndEvent
	#tag Event
		Sub Opening()
		  Var menu As New DesktopMenuItem
		  menu.AddMenu(New DesktopMenuItem("Text", "text"))
		  menu.AddMenu(New DesktopMenuItem("Line", "line"))
		  menu.AddMenu(New DesktopMenuItem("Bild", "pict"))
		  
		  Var btn As DesktopToolbarButton = Me.tbNewElem
		  btn.Menu = menu
		End Sub
	#tag EndEvent
	#tag Event
		Sub MenuItemSelected(item As DesktopToolbarItem, selectedItem As DesktopMenuItem)
		  Using PdfTemplate
		  
		  Select Case selectedItem.Tag
		    
		  Case "line"
		    EditLineElem(New CanvasElementLine, True)
		    
		  Case "text"
		    EditTextElem(New CanvasElementText, True)
		    
		  Case "pict"
		    EditPictureElem(New CanvasElementPicture, True)
		    
		  End
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CanvasRulerH
	#tag Event
		Sub Paint(g As Graphics, areas() As Rect)
		  Using PdfTemplate
		  
		  // Hintergrund
		  g.DrawingColor = &cD6D6D600
		  g.FillRectangle(0, 0, g.Width, g.Height)
		  
		  // Lineal
		  g.DrawingColor = &c91919100
		  g.FontName = "Arial"
		  g.FontSize = 9
		  For m As Integer = App.PdfSvc.GridSizeMm To App.PdfSvc.PxToMm(g.Height) Step App.PdfSvc.GridSizeMm * 2
		    Var y As Integer = App.PdfSvc.MmToPx(m)
		    g.DrawLine(0, y, g.Width, y)
		    g.DrawText(Str(m), 0, y)
		  Next
		End Sub
