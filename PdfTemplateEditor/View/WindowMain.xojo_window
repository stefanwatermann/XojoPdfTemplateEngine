#tag Events Canvas1
	#tag Event
		Sub Paint(g As Graphics, areas() As Rect)
		  Using PdfTemplate
		  
		  // Hintergrund
		  g.DrawingColor = Color.White
		  g.FillRectangle(0, 0, g.Width, g.Height)
		  
		  // Seiten Rahmen
		  g.DrawingColor = &cEBEBEB00
		  g.DrawLine(App.PdfSvc.MmToPx(PageWidthMm), 0, App.PdfSvc.MmToPx(PageWidthMm), g.Height)
		  g.DrawLine(0, App.PdfSvc.MmToPx(PageHeightMm), g.Width, App.PdfSvc.MmToPx(PageHeightMm))
		  
		  // Raster
		  g.DrawingColor = &c94520000
		  For my As Integer = App.PdfSvc.GridSizeMm To App.PdfSvc.PxToMm(g.Height) Step App.PdfSvc.GridSizeMm
		    For mx As Integer = App.PdfSvc.GridSizeMm To App.PdfSvc.PxToMm(g.Width) Step App.PdfSvc.GridSizeMm
		      Var x As Integer = App.PdfSvc.MmToPx(mx)
		      Var y As Integer = App.PdfSvc.MmToPx(my)
		      g.DrawLine(x, y + 1, x, y + 1)
		    Next
		  Next
		  
		  // Elemente
		  For Each e As CanvasElement In App.PdfSvc.CanvasElements
		    
		    // Element Text
		    If e IsA CanvasElementText Then
		      Var te As CanvasElementText = CanvasElementText(e)
		      te.Draw(g)
		    End
		    
		    // Element Line
		    If e IsA CanvasElementLine Then
		      Var te As CanvasElementLine = CanvasElementLine(e)
		      te.Draw(g)
		    End
		    
		    // Element Picture
		    If e IsA CanvasElementPicture Then
		      Var te As CanvasElementPicture = CanvasElementPicture(e)
		      te.Draw(g)
		    End
		    
		    // Selectionframe
		    If e = SelectedElement Then
		      g.DrawingColor = &cFFD47900
		      g.PenSize = 2
		      g.LineDash = Nil
		      g.DrawRectangle(e.GetBounds.Left, e.GetBounds.Top, e.GetBounds.Width, e.GetBounds.Height)
		    End
		    
		  Next
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function MouseDown(x As Integer, y As Integer) As Boolean
		  Self.LastMouseDownPos = New Point(x, y)
		  Self.SelectElement(x, y)
		  Canvas1.Refresh
		  Return True
		End Function
	#tag EndEvent
	#tag Event
		Sub MouseDrag(x As Integer, y As Integer)
		  If LastMouseDownPos <> Nil And SelectedElement <> Nil Then
		    System.DebugLog("X=" + Str(x) + ", Y=" + Str(y) + ", lastX=" + Str(Self.LastMouseDownPos.X) + ", lastY=" + Str(Self.LastMouseDownPos.Y))
		    MoveElement(SelectedElement, x, y)
		  End
		End Sub
	#tag EndEvent
	#tag Event
		Sub DoublePressed(x As Integer, y As Integer)
		  Using PdfTemplate
		  
		  EditCanvasElement()
		End Sub
	#tag EndEvent
#tag EndEvents
