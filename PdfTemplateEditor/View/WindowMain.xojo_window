#tag WindowCode
	#tag Event
		Function CancelClosing(appQuitting As Boolean) As Boolean
		  Var cancel As Boolean = False
		  
		  If IsDirty Then
		    If MessageAskBox.Show("Beenden", "Es sind ungespeicherte Daten vorhanden. Trotzdem beenden?", MessageDialog.IconTypes.Stop) = 0 Then
		      cancel = True
		    End
		  End
		  
		  Return cancel
		End Function
	#tag EndEvent

	#tag Event
		Sub Closing()
		  Try
		    
		    // try delete temporary pdf file
		    Var f As folderitem = SpecialFolder.Temporary.Child("tmp.pdf")
		    If f <> Nil And f.Exists Then
		      f.Remove
		    End
		    
		  Catch e As RuntimeException
		    // ignore exception
		    
		  End
		End Sub
	#tag EndEvent

	#tag Event
		Sub Opening()
		  Using PdfTemplate
		  
		End Sub
	#tag EndEvent


	#tag MenuHandler
		Function mnAbout() As Boolean Handles mnAbout.Action
		  MessageBox("PDF Template Designer" + EndOfLine + "Version " + str(App.MajorVersion) + "." + str(App.MinorVersion) + EndOfLine + "(c)2025 Stefan Watermann, Germany")
		  Return True
		End Function
	#tag EndMenuHandler

	#tag MenuHandler
		Function mnSaveAs() As Boolean Handles mnSaveAs.Action
		  Var f As FolderItem = FolderItem.ShowSaveFileDialog(FtgPdfTemplateFile.All, "template.ptf")
		  If f <> Nil Then
		    CurrentFile = f
		    SaveTemplateFile
		  End
		  
		  Return True
		End Function
	#tag EndMenuHandler


	#tag Method, Flags = &h21
		Private Sub CreatePdf()
		  Using PdfTemplate
		  
		  Var params As New Dictionary
		  params.Value("Date") = DateTime.Now.ToString(DateTime.FormatStyles.Medium, DateTime.FormatStyles.None)
		  params.Value("LongDate") = DateTime.Now.ToString(DateTime.FormatStyles.Long, DateTime.FormatStyles.None)
		  params.Value("Time") = DateTime.Now.ToString(DateTime.FormatStyles.None, DateTime.FormatStyles.Medium)
		  
		  Var helper As New PdfHelper
		  helper.CreateA4PdfDoc
		  helper.DrawElements(App.PdfSvc.CanvasElements, params)
		  
		  Var f As FolderItem = SpecialFolder.Temporary.Child("tmp.pdf")
		  File.WriteAllBytes(f, helper.PdfDoc.ToData)
		  f.Open
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub EditCanvasElement()
		  Using PdfTemplate
		  
		  If SelectedElement <> Nil Then
		    
		    If SelectedElement IsA CanvasElementText Then
		      EditTextElem(CanvasElementText(SelectedElement))
		    End
		    
		    If SelectedElement IsA CanvasElementLine Then
		      EditLineElem(CanvasElementLine(SelectedElement))
		    End
		    
		    If SelectedElement IsA CanvasElementPicture Then
		      EditPictureElem(CanvasElementPicture(SelectedElement))
		    End
		    
		  End
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub EditLineElem(elem as PdfTemplate.CanvasElementLine, add as boolean = false)
		  Var dlg As New DialogLineElem(elem)
		  dlg.ShowModal(Self)
		  If dlg.DialogOk And elem.WidthMm > 0 Then
		    If add Then
		      App.PdfSvc.CanvasElements.Add(elem)
		    End
		    elem.Init
		    Canvas1.Refresh
		    IsDirty = true
		  End
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub EditPictureElem(elem as PdfTemplate.CanvasElementPicture, add as Boolean = false)
		  Var dlg As New DialogPictureElem(elem)
		  dlg.ShowModal(Self)
		  If dlg.DialogOk And elem.Pic <> Nil Then
		    If add Then
		      App.PdfSvc.CanvasElements.Add(elem)
		    End
		    elem.Init
		    Canvas1.Refresh
		    IsDirty = true
		  End
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub EditTextElem(elem as PdfTemplate.CanvasElementText, add as Boolean = false)
		  Var dlg As New DialogTextElem(elem)
		  dlg.ShowModal(Self)
		  If dlg.DialogOk And elem.Text <> "" Then
		    If add Then
		      App.PdfSvc.CanvasElements.Add(elem)
		    End
		    elem.Init
		    Canvas1.Refresh
		    IsDirty = true
		  End
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub MoveElement(e as PdfTemplate.CanvasElement, x as integer, y as integer)
		  Using PdfTemplate
		  
		  If e <> Nil Then
		    
		    // Verschiebung
		    Var dx As Integer = x - LastMouseDownPos.X
		    Var dy As Integer = y - LastMouseDownPos.Y
		    
		    // Snap to Grid
		    dx = App.PdfSvc.MmToPx(Snap(App.PdfSvc.PxToMm(dx), App.PdfSvc.GridSizemm))
		    dy = App.PdfSvc.MmToPx(Snap(App.PdfSvc.PxToMm(dy), App.PdfSvc.GridSizemm))
		    
		    // neue Position
		    e.Xpx = SelectedElementLastPos.X + dx
		    e.Ypx = SelectedElementLastPos.Y + dy
		    
		    // Refresh
		    Canvas1.Refresh
		    
		    SetStatus1()
		    
		    IsDirty = true
		  End
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub NewTemplate()
		  Using PdfTemplate
		  
		  If MessageAskBox.Show("Ja", "Soll ein neues Template angelegt werden?", MessageDialog.IconTypes.Question) = 1 Then
		    App.PdfSvc.InitPdfTemplate
		    Canvas1.Refresh
		    IsDirty = false
		  End
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub OpenTemplateFile()
		  CurrentFile = FolderItem.ShowOpenFileDialog(FtgPdfTemplateFile.All)
		  
		  If CurrentFile <> Nil And CurrentFile.Exists Then
		    App.PdfSvc.Load(CurrentFile)
		    UpdateWindowTitle()
		    RefreshCanvas
		    tbGridSize.SelectRowWithText(Str(App.PdfSvc.GridSizeMm))
		    IsDirty = false
		  End
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub RefreshCanvas()
		  Canvas1.Refresh
		  CanvasRulerV.Refresh
		  CanvasRulerH.Refresh
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub RemoveCanvasElement()
		  Using PdfTemplate
		  
		  If MessageAskBox.Show("Löschen", "Soll das gewählte Element entfernt werden?", MessageDialog.IconTypes.Question) = 1 Then
		    App.PdfSvc.RemoveCanvasElement(SelectedElement)
		    Canvas1.Refresh
		  End 
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub SaveTemplateFile()
		  If CurrentFile = Nil Or Not CurrentFile.Exists Then
		    CurrentFile = FolderItem.ShowSaveFileDialog(FtgPdfTemplateFile.All, "template.ptf")
		  End
		  
		  If CurrentFile <> Nil Then
		    App.PdfSvc.Save(CurrentFile)
		    UpdateWindowTitle()
		    IsDirty = false
		  End
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub SelectElement(x as integer, y as integer)
		  Using PdfTemplate
		  
		  ToolbarMainWindow1.tbEditElem.Enabled = False
		  ToolbarMainWindow1.tbDeleteElem.Enabled = False
		  
		  Self.SelectedElement = Nil
		  For Each e As CanvasElement In App.PdfSvc.CanvasElements
		    If e.GetBounds.Contains(x, y) Then
		      Self.SelectedElement = e
		      Self.SelectedElementLastPos = New Point(e.Xpx, e.Ypx)
		      ToolbarMainWindow1.tbEditElem.Enabled = True
		      ToolbarMainWindow1.tbDeleteElem.Enabled = True
		    End
		  Next
		  
		  SetStatus1()
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub SetStatus1()
		  If Self.SelectedElement = Nil Then
		    lbStatus1.Text = ""
		  Else
		    lbStatus1.Text = "X=" + Str(SelectedElement.Xmm) + " Y=" + Str(SelectedElement.Ymm)
		  End
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub ShowFontDefs()
		  Var dlg As New DialogFontDefs
		  dlg.ShowModal(self)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Function Snap(x as Integer, grid as Integer) As Integer
		  return x - (x mod grid)
		End Function
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub UpdateWindowTitle(dirty as string = "")
		  If CurrentFile <> Nil Then
		    Self.Title = kWindowTitle + " | " + CurrentFile.Name + " " + dirty
		  Else
		    Self.Title = kWindowTitle + " " + dirty
		  End
		End Sub
	#tag EndMethod


